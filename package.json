{
  "name": "yaci-explorer",
  "version": "1.0.0",
  "description": "Modern block explorer for Cosmos SDK chains with EVM support",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "panda codegen && bun ./dev.ts",
    "build": "panda codegen && NODE_ENV=production bun ./build.ts",
    "start": "bun --bun serve ./dist",
    "preview": "bun run build && bun run start",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "backend:up": "docker compose -f docker/docker-compose.yml up postgres postgrest yaci",
    "panda:watch": "panda codegen --watch",
    "clean": "rm -rf dist styled-system",
    "clean:all": "rm -rf dist node_modules styled-system",
    "reinstall": "bun run clean:all && bun install",
    "configure:env": "bash ./scripts/configure-env.sh",
    "deploy:build": "bun install && bun run typecheck && bun run build",
    "redeploy:systemd": "bash ./scripts/redeploy-systemd.sh"
  },
  "dependencies": {
    "@ark-ui/react": "5.29.1",
    "@coinbase/wallet-sdk": "^4.3.7",
    "@gemini-wallet/core": "^0.3.2",
    "@metamask/sdk": "^0.34.0",
    "@reown/appkit": "^1.8.17",
    "@reown/appkit-adapter-wagmi": "^1.8.17",
    "@tanstack/react-query": "5.90.10",
    "@walletconnect/ethereum-provider": "^2.23.4",
    "bech32": "^2.0.0",
    "buffer": "^6.0.3",
    "date-fns": "4.1.0",
    "echarts": "5.6.0",
    "echarts-for-react": "3.0.5",
    "isbot": "5.1.32",
    "lucide-react": "0.554.0",
    "porto": "^0.2.37",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-router": "7.9.6",
    "viem": "^2.45.1",
    "wagmi": "^3.4.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@pandacss/dev": "^1.6.0",
    "@pandacss/preset-base": "1.5.1",
    "@park-ui/panda-preset": "0.43.1",
    "@types/bun": "^1.2.14",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "postcss": "8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-load-config": "^6.0.1",
    "typescript": "5.9.3"
  },
  "packageManager": "bun@1.3.2"
}
